    <script>
    // On thumbnail click, change image source in modal dialog
    var modalPhoto = document.getElementById('modal-photo');
    var modalLabel = document.getElementById('modal-label');
    $("a[data-toggle='modal']").click(function(event) {
        var imgSrc = event.target.src;
        var imgAlt = event.target.getAttribute('alt');
        imgSrc = imgSrc.replace('thumb', 'large');
        modalPhoto.src = imgSrc;
        modalPhoto.setAttribute('alt', imgAlt);
        modalLabel.innerHTML = imgAlt;
    });
    $("a[data-toggle='modal']").blur(function() {
        modalPhoto.src = 'img/loading.gif';
        modalPhoto.setAttribute('alt', 'Loading image');
    });
    </script>
